{
  "setup": {
    "name": "language-trainer",
    "version": "0.1.0",
    "author": "Eduardo B",
    "author_email": "eduardob999@example.com",
    "description": "Multiple language trainer - A CLI application for language learning",
    "long_description_file": "README.md",
    "long_description_content_type": "text/markdown",
    "url": "https://github.com/eduardob999/language-trainer",
    "packages": [
      "language_trainer",
      "language_trainer.core",
      "language_trainer.modules"
    ],
    "classifiers": [
      "Development Status :: 3 - Alpha",
      "Intended Audience :: Education",
      "Topic :: Education",
      "License :: OSI Approved :: MIT License",
      "Programming Language :: Python :: 3",
      "Programming Language :: Python :: 3.8",
      "Programming Language :: Python :: 3.9",
      "Programming Language :: Python :: 3.10",
      "Programming Language :: Python :: 3.11"
    ],
    "python_requires": ">=3.8",
    "install_requires": [],
    "entry_points": {
      "console_scripts": [
        "language-trainer=language_trainer.main:main"
      ]
    },
    "include_package_data": true,
    "package_data": {
      "language_trainer": [
        "data/json/*.json",
        "data/csv/*.csv"
      ]
    }
  },
  "app": {
    "name": "Language Trainer",
    "version": "0.1.0",
    "data_path": "source/data",
    "backup_path": "backup",
    "temp_path": "temp"
  }
}
